<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">

        <h3>Events</h3>

        <div class="container">

            <div class="row">
                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">

                    <div class="boxed">
                        {{#link-to 'events.new'}}<button type="button" class="btn btn-xs btn-primary">New Event</button>{{/link-to}}
                        <!-- Title Bart Start -->
                        <div class="title-bar white">
                            <h4>Events</h4>
                        </div>
                        <!-- Title Bart End -->

                        <div class="inner no-radius">
                            <ul class="socket-list">
                                {{#each events}}
                                    <li>
                                        <span>
                                            {{render "event" this}}
                                        </span>
                                    </li>
                                {{/each}}
                            </ul>
                        </div>


                    </div>
                </div>
            </div>
        </div>
    </div>
</div>