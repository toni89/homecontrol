<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">

<h3>General</h3>

<div class="container">

<div class="row">

<div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">


    <div class="boxed">

        <!-- Title Bart Start -->
        <div class="title-bar white">
            <h4>Generated Content</h4>
            <ul class="actions">

                <li><a href="#" class="close-box"><i class="fa fa-chevron-up"></i></a></li>
                <li><a href="#" class="remove-box"><i class="fa fa-times-circle-o"></i></a></li>
            </ul>
        </div>
        <!-- Title Bart End -->

        <div class="inner no-radius">

            {{#each controller}}
                {{#view App.TriggerSocketView}}<button type="button" class="btn btn-lg btn-primary">{{name}} - {{code}}</button>{{/view}}
            {{/each}}

        </div>

    </div>


<div class="boxed">

    <!-- Title Bart Start -->
    <div class="title-bar white">
        <h4>Wohnzimmer</h4>
        <ul class="actions">
            <li><a href="#" class="close-box"><i class="fa fa-chevron-up"></i></a></li>
            <li><a href="#" class="remove-box"><i class="fa fa-times-circle-o"></i></a></li>
        </ul>
    </div>
    <!-- Title Bart End -->

    <div class="inner no-radius">
        <button type="button" class="btn btn-lg btn-danger">Alle</button>
        <button type="button" class="btn btn-lg btn-primary">Fernseher</button>
        <button type="button" class="btn btn-lg btn-primary">Stehlampe</button>
        <button type="button" class="btn btn-lg btn-primary">Aquarium</button>

    </div>

</div>





<div class="boxed">

    <!-- Title Bart Start -->
    <div class="title-bar white">
        <h4>Whirlpool</h4>
        <ul class="actions">

            <li><a href="#" class="close-box"><i class="fa fa-chevron-up"></i></a></li>
            <li><a href="#" class="remove-box"><i class="fa fa-times-circle-o"></i></a></li>
        </ul>
    </div>
    <!-- Title Bart End -->

    <div class="inner no-radius">

        <button type="button" class="btn btn-lg btn-danger">Alle</button>
        {{#link-to 'settings'}}<button type="button" class="btn btn-lg btn-primary">Pumpe</button>{{/link-to}}

        <button type="button" class="btn btn-lg btn-primary">Heizung</button>

    </div>

</div>
</div>

</div>

</div>

</div>